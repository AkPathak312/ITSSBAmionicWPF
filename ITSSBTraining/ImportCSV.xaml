<Window x:Class="ITSSBTraining.ImportCSV"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ITSSBTraining"
        mc:Ignorable="d"
        Title="Import Schedules" Height="200" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <StackPanel HorizontalAlignment="Center" Margin="30" Orientation="Horizontal">
            <TextBox x:Name="txtFileName" Width="200" Padding="2" Margin="10"/>
            <Button Click="Button_Click" Content="Upload" Margin="10" Padding="2"/>
        </StackPanel>
        <TextBlock x:Name="txtData" Text="Data" VerticalAlignment="Bottom" HorizontalAlignment="Center"/>
    </Grid>
</Window>
